{
  "name": "{{ pluginName }}",
  "scripts": {
    "build": "npm-run-all {{^onlyWebapp}}build:backend{{/onlyWebapp}} {{#module.webapp}}build:webapp{{/module.webapp}}",
    {{^onlyWebapp}}
    "build:backend": "node build.js",
    {{/onlyWebapp}}
    {{#module.webapp}}
    "build:webapp": "rollup -c",
    {{/module.webapp}}
    "clean": "rimraf dist"
  },
  {{#module.webapp}}
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  {{/module.webapp}}
  "devDependencies": {
    {{^onlyWebapp}}
    {{/onlyWebapp}}
    {{#module.webapp}}
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.77.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "typescript": "^4.7.4",
    "typescript-plugin-css-modules": "^3.4.0",
    {{/module.webapp}}
    "@home-gallery/types": "1.0.0",
  }
